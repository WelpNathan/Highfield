@model InvigilatorModel

@{

}

@for (int i = 0; i < Model.learners.Count(); i++)
{
    <tr>

        <td id="@i">@Model.learners[i].name[0] @Model.learners[i].name[1]</td>
        @{string time = i.ToString() + "time"; }
        <td id="@time">@Model.learners[i].time</td>
        <td>
            <form asp-page-handler="select" method="get" autocomplete="off">
                <button class="fas fa-arrow-alt-circle-right" onclick="buttonRight(@i.ToString(), @time.ToString())"></button>
            </form>
        </td>


        <!----<i class="fas fa-arrow-alt-circle-right"></i>-->
    </tr>
}